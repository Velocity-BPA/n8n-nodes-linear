{
  "name": "n8n-nodes-linear",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Linear providing 15 resources and 100+ operations for issue tracking, project management, and team collaboration. Includes full GraphQL API integration with webhook triggers.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-linear.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-linear#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-linear/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "linear",
    "issue-tracking",
    "project-management",
    "graphql",
    "workflow-automation",
    "sprint-planning",
    "bug-tracking"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LinearApi.credentials.js",
      "dist/credentials/LinearOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Linear/Linear.node.js",
      "dist/nodes/Linear/LinearTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "copyfiles -u 2 \"nodes/**/*.svg\" \"nodes/**/*.png\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
